export ANYFRAME_SELECTOR_HEADER="Work"
export ANYFRAME_FILTER_REPO="work"

anyframe-source-filter-repository \
  | anyframe-selector-fzf --prompt !forks \
  | awk -F'\n' "{ print \"\\\"\" \"$(ghq root)/${ANYFRAME_FILTER_REPO}/\" \$1 \"\\\"\" }" \
  | anyframe-action-execute cd --

export ANYFRAME_SELECTOR_HEADER=""
export ANYFRAME_FILTER_REPO=""

# Local Variables:
# mode: Shell-Script
# End:
# vim: ft=zsh
